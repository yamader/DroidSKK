<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:keyWidth="10%p"
    app:horizontalGap="0dip"
    app:verticalGap="0dip"
	app:keyHeight="8%p"
    >

    <Row>
        <Key app:codes="49" app:keyLabel="1" app:keyEdgeFlags="left"/>
        <Key app:codes="50" app:keyLabel="2"/>
        <Key app:codes="51" app:keyLabel="3"/>
        <Key app:codes="52" app:keyLabel="4"/>
        <Key app:codes="53" app:keyLabel="5"/>
        <Key app:codes="54" app:keyLabel="6"/>
        <Key app:codes="55" app:keyLabel="7"/>
        <Key app:codes="56" app:keyLabel="8"/>
        <Key app:codes="57" app:keyLabel="9"/>
        <Key app:codes="48" app:keyLabel="0" app:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key app:codes="33" app:keyLabel="!" app:keyEdgeFlags="left"/>
        <Key app:codes="64" app:keyLabel="\@"/>
        <Key app:codes="35" app:keyLabel="\#"/>
        <Key app:codes="36" app:keyLabel="$"/>
        <Key app:codes="37" app:keyLabel="%"/>
        <Key app:codes="94" app:keyLabel="^"/>
        <Key app:codes="38" app:keyLabel="&amp;"/>
        <Key app:codes="42" app:keyLabel="*"/>
        <Key app:codes="40" app:keyLabel="("/>
        <Key app:codes="41" app:keyLabel=")" app:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key app:codes="-1" app:keyIcon="@drawable/ic_keyboard_shift"
                app:keyWidth="15%p" app:isModifier="true"
                app:isSticky="true" app:keyEdgeFlags="left"/>
        <Key app:codes="45" app:keyLabel="-" />
        <Key app:codes="61" app:keyLabel="="/>
        <Key app:codes="91" app:keyLabel="["/>
        <Key app:codes="93" app:keyLabel="]"/>
        <Key app:codes="59" app:keyLabel=";"/>
        <Key app:codes="39" app:keyLabel="\'" />
        <Key app:codes="47" app:keyLabel="/"/>
        <Key app:codes="-5" app:keyIcon="@drawable/ic_keyboard_backspace"
                app:keyWidth="15%p" app:keyEdgeFlags="right"
                app:isRepeatable="true"/>
    </Row>

    <Row  app:rowEdgeFlags="bottom">
        <Key app:codes="-1008" app:keyLabel="JP"
                app:keyWidth="20%p" app:keyEdgeFlags="left"/>
        <Key app:codes="-1010" app:keyLabel="ABC" app:keyWidth="15%p"/>
        <Key app:codes="32" app:keyIcon="@drawable/ic_keyboard_spacebar"
                app:keyWidth="30%p" app:isRepeatable="true"/>
        <Key app:codes="46,44" app:keyLabel=". ,"
                app:keyWidth="15%p"/>
        <Key app:codes="-1011" app:keyIcon="@drawable/ic_keyboard_return"
                app:keyWidth="20%p" app:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
